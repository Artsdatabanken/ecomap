{
  "name": "ecomap",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "ci_test": "node scripts/test.js --env=jsdom --coverage --testResultsProcessor jest-junit",
    "coveralls": "npm run ci_test && cat coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "lint": "./node_modules/.bin/standard",
    "lint-fix": "./node_modules/.bin/standard --fix",
    "lint_old": "eslint -c .eslintrc src/**/*.js*",
    "lint-report": "standard | standard-reporter --checkstyle >lint-report.xml || exit 0",
    "lint-report_old": "eslint -c .eslintrc -f checkstyle -o lint-report.xml src/**/*.js* > eslint.log|| exit 0",
    "storybook": "start-storybook -p 9009 -s public",
    "test-storybook": "storyshots",
    "build-storybook": "build-storybook -s public -o build/storybook",
    "validate": "npm ls"
  },
  "dependencies": {
    "autoprefixer": "7.1.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-preset-react-app": "^3.0.1",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "color_ramps": "^1.0.4",
    "css-loader": "0.28.4",
    "deck.gl": "^4.0.6",
    "dotenv": "4.0.0",
    "eslint": "3.19.0",
    "eslint-config-react-app": "^1.0.5",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.34.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "mapbox-gl": "^0.38.0",
    "material-ui": "^0.18.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.6",
    "promise": "7.1.1",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-color": "^2.11.7",
    "react-dev-utils": "^3.0.2",
    "react-dom": "^15.5.4",
    "react-error-overlay": "^1.0.9",
    "react-mapbox-gl": "^2.0.3",
    "react-router-dom": "^4.0.0",
    "react-tap-event-plugin": "^2.0.1",
    "recharts": "^1.0.0-alpha.0",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.3",
    "url-loader": "0.5.9",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.5.0",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@storybook/addon-storyshots": "^3.0.0",
    "@storybook/react": "^3.0.1",
    "babel-eslint": "^7.2.3",
    "canvas": "^1.6.5",
    "canvas-prebuilt": "^1.6.5-prerelease.1",
    "coveralls": "^2.13.1",
    "flow-bin": "^0.49.1",
    "jest-junit": "^1.2.0",
    "pixelmatch": "^4.0.2",
    "precommit-hook": "^3.0.0",
    "react-test-renderer": "^15.4.2",
    "standard": "^10.0.2",
    "standard-reporter": "^1.0.5",
    "storybook-addon-material-ui": "^0.7.9"
  },
  "homepage": ".",
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "it",
      "expect",
      "fetch"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "pre-commit": [
    "ci_test",
    "lint"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
